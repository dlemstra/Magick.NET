    <releaseNotes>https://github.com/dlemstra/Magick.NET/releases/tag/14.9.1</releaseNotes>
    <copyright>Copyright 2013-2025 Dirk Lemstra</copyright>
    <tags>ImageMagick Magick.NET Image Convert Resize Draw Effects</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.6.2">
        <dependency id="Magick.NET.Core" version="14.9.1" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Magick.NET.Core" version="14.9.1" />
        <dependency id="System.Drawing.Common" version="8.0.20" />
      </group>
      <group targetFramework="net8.0">
        <dependency id="Magick.NET.Core" version="14.9.1" />
        <dependency id="System.Drawing.Common" version="8.0.20" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Drawing" />
    </frameworkAssemblies>
